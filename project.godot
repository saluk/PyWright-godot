[application]

config/name="GodotWright"
run/main_scene="res://Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="GodotWright"
config/icon="res://icon.png"

[autoload]

Commands="*res://System/WrightScript/Commands.gd"
ZLayers="*res://System/Scene/ZLayers.gd"
MusicPlayer="*res://System/Audio/MusicPlayer.gd"
SoundPlayer="*res://System/Audio/SoundPlayer.gd"
WSExpression="*res://System/WrightScript/WSExpression.gd"
DirectoryCache="*res://System/Files/DirectoryCache.gd"
ObjectFactory="*res://System/Scene/ObjectFactory.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/shadowed_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false

[display]

window/size/width=512
window/size/height=384
window/size/test_width=512
window/size/test_height=384
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

limits=false

[importer_defaults]

texture={
"detect_3d": false,
"flags/anisotropic": true
}

[input]

quickload={
"deadzone": 0.5,
"events": [  ]
}

[network]

limits/debugger_stdout/max_chars_per_second=404800
limits/debugger_stdout/max_messages_per_frame=10000

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
